{
  "presets": [
    "@babel/preset-typescript",
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "chrome": 55,
          "edge": 11,
          "firefox": 60,
          "safari": 11,
          "node": "12.18"
        },
        "loose": false
      }
    ]
  ],
  "plugins": [["@babel/plugin-proposal-class-properties", { "loose": true }]]
}
