  "license": "SEE LICENSE IN LICENSE.md",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.1.0",
    "serverless": "^2.8.0",
    "serverless-dotenv-plugin": "^3.1.0",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-offline": "^6.8.0",
    "serverless-plugin-monorepo": "^0.9.0",
    "serverless-stage-manager": "^1.0.5",
    "serverless-webpack": "^5.3.5",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.5",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2"
  },
  "scripts": {
    "start": "serverless offline",
    "build": "echo \"api: No build script\"",
    "clean": "rm -rf .webpack lib",
    "aws:profile:set": "serverless config credentials --provider aws --key $AWS_ACCESS_KEY_ID --secret $AWS_SECRET_ACCESS_KEY --profile picnic -o",
    "deploy:staging": "serverless deploy --stage staging",
    "deploy:staging:info": "serverless info --stage staging",
    "deploy:production": "serverless deploy --stage production",
    "deploy:production:info": "serverless info --stage production",
    "dynamo:install": "serverless dynamodb install --stage dev",
    "dynamo:start": "serverless dynamodb start --stage dev",
    "dynamo:migrate": "serverless dynamodb migrate --stage dev"
  },
  "dependencies": {
    "@amfa-team/types": "^0.1.0",
    "@sentry/node": "^5.27.1",
    "@types/aws-lambda": "^8.10.64",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.779.0",
    "dynamoose": "^2.3.0",
    "node-fetch": "^2.6.1",
    "source-map-support": "^0.5.19",
    "ts.data.json": "^1.4.0",
    "uuid": "^8.3.1"
  }
