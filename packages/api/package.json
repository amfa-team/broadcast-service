    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  },
  "scripts": {
    "start": "serverless offline --stage dev",
    "build": "tsc -b",
    "clean": "tsc -b --clean && rm -rf .webpack lib",
    "aws:profile:set": "serverless config credentials --provider aws --key $AWS_ACCESS_KEY_ID --secret $AWS_SECRET_ACCESS_KEY --profile picnic -o",
    "deploy:staging": "serverless deploy --stage staging",
    "deploy:staging:info": "serverless info --stage staging",
    "deploy:production": "serverless deploy --stage production",
    "deploy:production:info": "serverless info --stage production",
    "dynamo:install": "serverless dynamodb install --stage dev",
    "dynamo:start": "serverless dynamodb start --stage dev",
    "dynamo:migrate": "serverless dynamodb migrate --stage dev"
  },
  "dependencies": {
    "@amfa-team/broadcast-service-types": "0.0.0",
    "@sentry/serverless": "^6.1.0",
    "@types/aws-lambda": "^8.10.72",
